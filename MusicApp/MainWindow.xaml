<Window x:Class="MusicApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="20">
            <Button Content="Go to Search" Width="120" Height="40"  Click="GoToSearch"/>
            <Button Content="Go to Chat" Width="120" Height="40" Click="GoToChat"/>
            <Button Content="Go to Profile" Width="120" Height="40" Click="GoToProfile"/>
            
        </StackPanel>
    </Grid>
</Window>
